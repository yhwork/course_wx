.head_img {
  position: relative;
}

.head_img .rank_img {
  position: absolute;
  top: 0;
  right: 0;
  width: 20rpx;
  height: 20rpx;
}

.reserves {
  border: 1px solid red;
}

.alist {
  padding: 10rpx 30rpx;
  margin-bottom: 10rpx;
  font-size: 30rpx;
  background-color: #fff;
}

.a_top {
  color: #0a4e7f;
  font-size: 32rpx;
}

.a_bottom {
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #888;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.a_bottom .b_right {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.a_bottom .b_right image {
  width: 30rpx;
  height: 20rpx;
  margin-left: 50rpx;
}

.c_video {
  position: relative;
}

.video_btn {
  height: 0;
  border-top: 25rpx solid transparent;
  border-left: 35rpx solid #fff;
  border-bottom: 25rpx solid transparent;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.choosechild {
  padding: 0 30rpx 10rpx;
}

.video_btn::after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  transform: translate(-85%, -50%);
  width: 70rpx;
  height: 70rpx;
  border: 2rpx solid #fff;
  border-radius: 50%;
}

.a_center_imgvideo {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  align-items: center;
  /* margin: 10rpx 0; */
}

.a_center_imgvideo>view {
  /* width: 32%;
