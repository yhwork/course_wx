.audios {
  width: 100%;
  position: relative;
  margin-bottom: 10rpx;
}

.audio {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 70rpx;
  border: 1rpx solid #ebebeb;
  border-radius: 10rpx;
  margin-bottom: 10rpx;
  background-color: #fbdeba;
}

.voice_animate {
  width: 100%;
  height: 100%;
  position: relative;
}

.name .notifylogo {
