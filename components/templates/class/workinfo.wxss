}

.playimg, .stopimg {
  width: 100%;
  height: 70rpx;
  position: relative;
}

.playimg image, .stopimg image {
  width: 24rpx;
  height: 36rpx;
  position: absolute;
  top: 50%;
  right: 10rpx;
  transform: translateY(-50%);
}

.proleft {
  position: absolute;
