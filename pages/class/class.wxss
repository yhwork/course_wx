page {
  background-color: #fff;
}
.headname{
  width: 180rpx;
}
.wrap {
  box-sizing: border-box;
}
.mall-wrap{
  width: 750rpx;
    background-color: #ffd596;

}
.txtname{
  max-width: 180rpx;
}
.search {
  margin-top: 10rpx;
  width: 100%;
  padding: 0 5%;
  position: fixed;
  top: 10rpx;
  left: 0;
}

.weui-icon-search_in-box {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 7%;
}

.search input {
  background: white;
  width: 90%;
  height: 80rpx;
  border: 2rpx solid #ccc;
  border-radius: 10rpx;
  padding-left: 10%;
  box-sizing: border-box;
  color: #e588e5e5;
}


.choose_child {
  padding: 10rpx 30rpx;
  background-color: #ffd596;
}

.imgchild, .mb image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 10rpx;
}

.mb {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  box-sizing: border-box;
}

.mblist {
  background-color: #fff;
  padding: 10rpx 30rpx;
  display: flex;
  align-items: center;
}

.mb image {
  margin-right: 20rpx;
}

.setclass {
  position: fixed;
  bottom: 20rpx;
  left: 2%;
  width: 96%;
  line-height: 80rpx;
  background-color: #f29219;
  color: white;
  font-size: 32rpx;
  text-align: center;
  height: 80rpx;
  border-radius: 10rpx;
}

.topimg {
  width: 100%;
  height: 300rpx;
}

.box {
  padding: 0 30rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
}

.title {
  color: #f29219;
  text-align: center;
  font-weight: 600;
}

.content {
  font-size: 28rpx;
}

.back {
  position: fixed;
  right: 30rpx;
  font-size: 28rpx;
  bottom: 30rpx;
  color: #f29219;
}


.message {
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 96rpx;
  width: 270rpx;
  /* margin: 0 auto; */
  background: rgba(102, 102, 102, 0.3);
  border-radius: 10rpx 0 0 10rpx;
  position: fixed;
  top: 115rpx;
  right: 0;
  padding-right: 20rpx;
  box-sizing: border-box;
}

.message .photo {
  width: 72rpx;
  height: 72rpx;
}

.message_num {
  color: #f29219;
}

.fa_angle_right {
  font-size: 50rpx;
  color: #c7c7cc;
}
.fl {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 28rpx;
}

.childmsg, .choose_child {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.left image {
  margin-right: 20rpx;
}

.right image {
  width: 35rpx;
  height: 35rpx;
}

.right text {
  font-size: 28rpx;
  color: #f29219;
  padding: 0 30rpx;
}

.section_lists_wrap {
  padding: 0 30rpx;
}

.section_lists_wrap1 {
  padding: 0 30rpx;
}

.section_lists_li image {
  width: 110rpx;
  height: 110rpx;
  margin-right: 20rpx;
}

.section_lists_li {
  border-bottom: 1px solid #e5e5e5;
  /* padding-left: 20rpx; */
  display: flex;
  align-items: center;
  padding: 10rpx 0;
}

.check_mark image {
  width: 26rpx;
  height: 18rpx;
}

.section_lists_ul {
  width: 100%;
  position: relative;
}

.section_lists_ul .active .class_name {
  color: #f29219;
}

.hide {
  display: none;
}

.show {
  display: block;
}

.addclass_btn_wrap {
  position: fixed;
  bottom: -10rpx;
  left: 45%;
}

.addclass image {
  width: 35rpx;
  height: 35rpx;
  border: 2rpx solid #f29219;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 20rpx;
}

.addclass {
  width: 50%;
  margin-top: 20rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-size: 30rpx;
  color: #000;
}

.addbox {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.btn {
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  border-radius: 0;
  background: #fff;
}

.listform {
  width: 100%;
  position: fixed;
  /* top: 0; */
  left: 0;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.fl text {
  color: #888;
  text-align: left;
  line-height: 40rpx;
  font-size: 26rpx;
  white-space: nowrap;
  overflow: hidden;
}

button::after {
  border: none;
}
