page {
  background-color: #fff;
}

.no_course {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-flow: column wrap;
  justify-content: center;
  align-items: center;
}

.course_img {
  height: 340rpx;
  width: 256rpx;
  position: relative;
  top: -26rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.course_img image:nth-child(3) {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 225rpx;
  height: 230rpx;
}

.course_img image:nth-child(1) {
  position: absolute;
  
  width: 237rpx;
  height: 130rpx;
  animation: myAnimaiton 2s infinite;
}

.course_img image:nth-child(2) {
  position: absolute;
 
  
  width: 192rpx;
  height: 108rpx;
  animation: myAnimaiton 3s infinite;
}
@keyframes myAnimaiton {
  0% {
    top: 10rpx;
    opacity: 1;
    right: 15rpx;
  }
  25% {
    visibility: hidden;
    opacity: 0;
  }
  50% {
    visibility: visible;
    top: 5rpx;
    right:25rpx;
    opacity: 1;
  }
  75% {
    visibility: hidden;
    opacity: 0;
  }
  100% {
    top: 10rpx;
    right: 15rpx;
    visibility: visible;
    opacity: 1;
  }
}
.no_course_txt {
    font-size:28rpx;
    font-family:PingFang SC;
