.list {
  margin-top: 20rpx;
}

.contents {
  padding: 40rpx 30rpx 0;
}

.basic_info, .basic_left {
  display: flex;
  align-items: center;
}

.photo {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.diary {
  padding-left: 90rpx;
  box-sizing: border-box;
}

.theme_cont>view {
  margin-bottom: 10rpx;
}

.img-box {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-content: center;
}

.imgs {
  width: 32%;
  height: 190rpx;
  border-radius: 10rpx;
}

.img-box .imgs:nth-child(3n+2) {
  margin: 0 2%;
}

.videobox {
  box-sizing: border-box;
  position: relative;
  width: 230rpx;
  height: 250rpx;
}

.videobox .img_content {
  width: 230rpx;
  height: 250rpx;
  border-radius: 10rpx;
}

.video_btn {
  height: 0;
  border-top: 25rpx solid transparent;
  border-left: 35rpx solid #fff;
  border-bottom: 25rpx solid transparent;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.video_btn::after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  transform: translate(-85%, -50%);
  width: 70rpx;
  height: 70rpx;
  border: 2rpx solid #fff;
  border-radius: 50%;
}
.voice_animate{
  width: 100%;
  height: 100%;
}
.playimg,.stopimg{
  width: 100%;
  height: 100%;
  position: relative;
}
.playimg image,.stopimg image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 50%;
  right: 30rpx;
  transform: translateY(-50%);
}
.proleft{
  position: absolute;
  top: 50%;
  right: 110rpx;
  transform: translateY(-50%);
}

.audio {
  display: flex;
  align-items: center;
  height: 70rpx;
  border: 1rpx solid #ebebeb;
  border-radius: 10rpx;
  margin-bottom: 10rpx;
  background-color: #fbdeba
}

.playOrstop {
  position: relative;
  width: 80rpx;
  height: 80rpx;
  border-radius: 100%;
  background-color: #f29219;
  margin-left: 20rpx;
}

.slid {
  width: 70%;
  height: 5rpx;
  background-color: #ccc;
  position: absolute;
  left: 80rpx;
  top: 50%;
  transform: translateY(-50%);
}
.slid view{
  height: 100%;
}


.diary_link {
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #f8f8f8;
  font-size: 0;
}

.link_left {
  display: flex;
  align-items: center;
}

.link_img {
  width: 90rpx;
  height: 90rpx;
}

.link_info {
  font-size: 30rpx;
  margin-left: 30rpx;
}

.link_left {
  display: flex;
  align-items: center;
}

.diary_link .member_num {
  font-size: 26rpx;
  color: #888;
}

.diary_link .member_num text {
  margin-right: 10rpx;
}

.diary_link .fa-angle-right {
  margin-right: 40rpx;
}

.fa-angle-right {
  font-size: 50rpx;
  color: #c7c7cc;
}


.pro{width:83%;display: flex;align-items: center}
/* .proleft{width:20%;} */
.proright {width: 20%;}
.procenter{width:80%;}
.proleft,.procenter,.proright{display: inline-block;}
.proleft{
  font-size: 24rpx;
  color: #888;
}

























page {
  background-color: #fff;
}

.wrap {
  font-size: 32rpx;
}

.selected,.name {
  color: #f29219;
}

.default {
  color: black;
}

.mycircle {
  position: absolute;
  top: 0;
  left: 0;
  font-size: 20rpx;
  background-color: rgba(0, 0, 0, 0.5);
  line-height: 30rpx;
  text-align: center;
  height: 32rpx;
  color: white;
  width: 60rpx;
}

.show {
  display: block;
}

.hidden {
  display: none;
}

.header_nav {
  width: 100%;
  height: 260rpx;
  background-color: #cbcbcb;
  text-align: center;
  position: relative;
}

#user {
  width: 130rpx;
  height: 130rpx;
  border-radius: 100%;
  margin-top: 10rpx;
}

.titleBar {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  padding: 30rpx 0 20rpx;
  margin: 0 30rpx;
  border-bottom: 2rpx solid #cbcbcb;
  box-sizing: border-box;
}

.titleBar>view {
  width: 50%;
  text-align: center;
}

.childs {
  position: absolute;
  bottom: -40rpx;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.childs>image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 100%;
  margin: 0 10rpx;
}

.lists_li {
  overflow: hidden;
  padding: 10rpx 0;
  border-bottom: 2rpx solid #cbcbcb;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.lists_li_left {
  float: left;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}

.lists_li_right {
  float: right;
}

.lists_li_left image {
  width: 120rpx;
  height: 100rpx;
}

.msg {
  margin-left: 20rpx;
  font-size: 30rpx;
}

.msg-right {
  font-size: 28rpx;
  color: #868686;
}

.signinNum {
  margin-left: 10rpx;
}

.arrow {
  width: 35rpx;
  height: 35rpx;
}

.choosed {
  border: 4rpx solid #f29219;
}

.node {
  border: 4rpx solid transparent;
}
