height:100rpx;
line-height: 100rpx;
font-size: 30rpx;
color:#000;
}
.panel_sure{
width: 50%;
text-align: center;
height:100rpx;
line-height: 100rpx;
font-size: 30rpx;
color:#F29219;
}

