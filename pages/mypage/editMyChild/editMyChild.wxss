/* footer s */

.edit_footer_wrap {
  display: flex;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 93rpx;
  line-height: 93rpx;
  border-top: 1rpx solid #e5e5e5;
  /* background-color: #fff; */
}
.edit_footer_wrap>view{
  width: 33.3%;
  text-align: center;
}

.edit_footer_left {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  color: rgba(136, 136, 136, 1);
}

.edit_footer_left text {
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  color: rgba(136, 136, 136, 1);
  margin-left: 20rpx;
}

.edit_footer_left image {
  width: 44rpx;
  height: 44rpx;
}

.edit_footer_right {
  display: flex;
  align-items: center;
  justify-content: center;
}

.edit_footer_right text {
  font-size: 26rpx;
  font-family: PingFangSC-Regular;
  color: rgba(136, 136, 136, 1);
  margin-left: 20rpx;
}

.edit_footer_right image {
  width: 44rpx;
  height: 44rpx;
}

/* 分享窗口提示 */

.share_panel_mask {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  top: 0;
  left: 0;
}

.show {
  display: block;
}

.hide {
  display: none;
}

.panel_container {
  width: 560rpx;
  height: 261rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  margin-left: 95rpx;
  margin-top: 45%;
  padding-top: 30rpx;
}

.panel_container_tit {
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  color: rgba(0, 0, 0, 1);
}

.panel_container_text {
  height: 88rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  color: rgba(136, 136, 136, 1);
  line-height: 88rpx;
  margin-top: 20rpx;
  text-align: center;
  border-bottom: 1px solid #e5e5e5;
}

.panel_cancel {
  width: 49%;
  border-right: 1px solid #e5e5e5;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #000;
}

.panel_sure {
  width: 50%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #f29219;
  background: #fff;
}

.panel_sure::after {
  border: none;
}

.share_mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}

.share_box {
  width: 100%;
  background: #fff;
  position: absolute;
  bottom: 0;
}

.addr {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  color: #576b95;
}

.addr image {
  width: 22rpx;
  height: 28rpx;
  margin-right: 20rpx;
}

.course_name {
  font-size: 34rpx;
  color: #222;
  margin-bottom: 10rpx;
}

.share_top {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 30rpx 20rpx 30rpx;
  font-size: 26rpx;
  color: #f29219;
  position: relative;
}

.share_top::after {
  content: "";
  width: 720rpx;
  height: 2rpx;
  background: #e5e5e5;
  position: absolute;
  bottom: 0;
}

.share_top .cancel {
  color: #b2b2b2;
}

.share_center {
  padding: 30rpx;
  font-size: 28rpx;
  /* color: #999; */
}

.date, .time_type {
  display: flex;
}

/* .start_date, .time_type .time {
  margin-right: 20rpx;
} */

.share_foot {
  display: flex;
  justify-content: space-between;
  padding: 30rpx;
  font-size: 26rpx;
  color: #353535;
}

.xcx, .weiXin {
  display: flex;
  align-items: center;
}

.xcx {
  margin-right: 70rpx;
}

.share_foot image {
  width: 90rpx;
  height: 90rpx;
  margin-right: 20rpx;
}

button {
  background: #fff;
  color: #222;
  font-size: 26rpx;
  padding: 0;
}

.button-hover {
  background: #fff;
}

button::after {
  border: none;
}

.hint {
  font-size: 28rpx;
  color: #333;
}

.share_hint {
  /* border-top: 2rpx solid #e5e5e5; */
  padding: 10rpx 30rpx;
  color: #666;
  font-size: 26rpx;
}

.share_hint image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}

.share_hint .title {
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
}

.hint_item {
  display: flex;
  line-height: 50rpx;
}

.gender {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
}

.gender_mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.3);
}

.gender_box {
  width: 100%;
  position: absolute;
  /* left: 50%; */
  left: 0;
  bottom: 0;
  /* transform: translateX(-50%); */
  z-index: 11;
  background: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.radio {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-top: 2rpx solid #dee1f4;
}

.button_submit {
  padding: 10rpx 90rpx;
  color: #f29219;
  font-weight: 700;
  text-align: right;
}
