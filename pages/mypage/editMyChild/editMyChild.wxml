          <view class='info_lists_li_title'>
            <image src='https://iforbao-prod.oss-cn-hangzhou.aliyuncs.com/public/assets/img/Carrier@2x.png'> </image>
            <text>权限管理</text>
          </view>
          <view class='info_lists_li_right'>
            <text class='textover'></text>
