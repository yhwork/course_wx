.voice_mask {
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 10;
}

.voice_box {
  height: 30%;
  width: 100%;
  background: #fff;
  position: absolute;
  bottom: 0;
  z-index: 11;
}

.voice_box .voice_btn {
  position: absolute;
  top: 20%;
  left: 50%;
  transform: translate(-50%);
  width: 150rpx;
  height: 150rpx;
  text-align: center;
  background-color: #f29219;
  border-radius: 100%;
  vertical-align: middle;
}

.voice_box_close .voice_box_close_imge {
  width: 50rpx;
  height: 50rpx;
}

.countDown {
  font-size: 24rpx;
  color: #222;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 0;
}

.voice_title {
  color: #222;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 75%;
}

.voice_box .voice_box_close {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0;
  right: 0;
  margin: 10rpx;
}

.voice_box_close .voice_box_close_imge {
  width: 50rpx;
  height: 50rpx;
}

.comment_audio {
  display: flex;
