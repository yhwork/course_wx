}

.voice_animate {
  display: flex;
  align-items: center;
  position: relative;
  height: 50rpx;
}

.audio_play image, .audio_stop image {
  width: 24rpx;
  height: 36rpx;
  position: absolute;
  top: 50%;
  right: 10rpx;
  transform: translateY(-50%);
}

