/* pages/classcircle/receiptType/receiptType.wxss */
page{
  background-color: #fff
}
.page{
  padding: 20rpx 30rpx;
}
.top{
  margin-bottom: 20rpx;
  font-size: 32rpx;
}
.classlist{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  color: #888;
  margin-bottom: 10rpx;
}
.bottomtab{
  position: fixed;
  bottom: 0rpx;
  left: 0;
  width: 100%;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  border: 2rpx solid #f29219;
  box-sizing: border-box;
}

.bottomtab .look,.bottomtab .set{
  font-size: 30rpx;
  height: 100%;
  line-height: 90rpx;
  text-align: center;
  border-radius: 0
}
.bottomtab .look{
  color: #f29219;
  background-color: #fff;
  width: 30%;
}
.bottomtab .set{
  background-color: #f29219;
  color: white;
  width: 70%;
}
.left image{
  width: 36rpx;
  height: 24rpx;
}
button::after{
  border: none;
}

.bot{
  color: #0a4e7f;
  margin-top: 30rpx;
}