.page__bd_list_class {
  display: flex;
  justify-content: center;
  padding-left: 30rpx;
  background: white;
  margin-bottom: 10rpx;
  height: 3em;
  min-height: 3em;
  overflow: hidden;
}

.weui-cell__hd {
  display: flex;
  align-items: center;
  font-size: 32rpx;
}
.weui-cell__bd button::after{
  border: none;
}
.weui-cell__bd button{
  background-color: #fff;
  padding: 0;
  margin: 0;
  height: 2.58823529em;
  min-height: 2.58823529em;
}
.classlogobox{
  height: 90rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.classlogo{
  width: 80rpx;
  height: 80rpx;
  border-radius: 80rpx;
}
.classid {
  text-align: right;
  padding-right: 62rpx;
  color: #999;
  height: 2.58823529em;
  min-height: 2.58823529em;
  line-height: 2.58823529em;
}

.delclass {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  border: 2rpx solid #ff4081;
  color: #ff4081;
  box-sizing: border-box;
  font-size: 30rpx;
}

