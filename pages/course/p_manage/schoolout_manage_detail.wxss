.tab_box {
  display: flex;
  justify-content: space-around;
  align-items: flex-end;
  background: #fafafa;
  padding: 30rpx 30rpx 24rpx 40rpx;
}

.tab_list {
  display: flex;
  flex: 1;
  justify-content: space-between;
  padding-right: 46rpx;
}

.tab_item {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.tab_right {
  font-size: 20rpx;
  color: #888;
  text-align: center;
}

.tab_right image {
  width: 32rpx;
  height: 28rpx;
  margin-bottom: 10rpx;
}

.tab_name {
  display: flex;
  align-items: center;
  position: relative;
  font-size: 28rpx;
}

.weui-tab__panel {
  padding-top: 0;
  margin-bottom: 110rpx;
}
.wx-nav{
  border-top: solid 1px rgba(0, 0, 0, 0.2);
  box-shadow: 1px 0px 1px rgba(0, 0, 0, 0.2);
}
.active {
  position: relative;
  color: #f29219;
}

.active::after {
  content: "";
  width: 46rpx;
  height: 6rpx;
  position: absolute;
  bottom: -14rpx;
  left: 50%;
  transform: translateX(-50%);
  background: #f29219;
}

.num {
  color: #000 !important;
}

.weui-cell__ft {
  text-align: left;
}

.share_mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}

.share_box {
  width: 100%;
  background: #fff;
  position: absolute;
  bottom: 0;
}

.addr {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  color: #576b95;
}

.addr image {
  width: 22rpx;
  height: 28rpx;
  margin-right: 20rpx;
}

.course_name {
  font-size: 34rpx;
  color: #222;
  margin-bottom: 10rpx;
}

.share_top {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 30rpx 20rpx 30rpx;
  font-size: 26rpx;
  color: #f29219;
  position: relative;
}

.share_top::after {
  content: "";
  width: 720rpx;
  height: 2rpx;
  background: #e5e5e5;
  position: absolute;
  bottom: 0;
}

.share_top .cancel {
  color: #b2b2b2;
}

.share_center {
  padding: 30rpx;
  font-size: 26rpx;
  color: #999;
}

.date, .time_type {
  display: flex;
}

.start_date, .time_type .time {
  margin-right: 50rpx;
}

.share_foot {
  display: flex;
  justify-content: space-between;
  padding: 30rpx;
  font-size: 26rpx;
  color: #353535;
}

.xcx, .weiXin {
  display: flex;
  align-items: center;
}

.xcx {
  margin-right: 70rpx;
}

.share_foot image {
  width: 90rpx;
  height: 90rpx;
  margin-right: 20rpx;
}

button {
  background: #fff !important;
  color: #222 !important;
  font-size: 26rpx;
}

button::after {
  border: none;
}

.map_img {
  width: 26rpx;
  height: 32rpx;
  margin-bottom: 8rpx;
  vertical-align: middle;
}

.weui-cell-nav{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #fff;
}
