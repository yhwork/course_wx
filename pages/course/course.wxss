  border-radius: 100%;
}

.rigth .shareimg,
.rigth .shareimg1 {
  width: 35rpx;
  height: 35rpx;
  margin-right: 10rpx;
}

.rigth .shareimg1 {
  transform: rotate(180deg);
}

.rigth1 {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.share1,
.share2 {
