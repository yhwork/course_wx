}

.backhome {
  position: fixed;
  bottom: 1rpx;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  /* padding: 10rpx 5rpx; */
  box-sizing: border-box;
  overflow-x: hidden;
  z-index: 999;
}

