.hint_img {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}

.button-hover {
  background: #f29219;
}

.delete_btn {
  width: 32rpx;
  height: 32rpx;
}

.changeschool {
  width: 38rpx;
  height: 36rpx;
}

.schoolname_box {
  width: 68%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.banji {
  position: relative;
}

.locationbox {
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.5);
}

.locationbox .box {
  position: absolute;
  top: 35%;
  left: 50%;
  transform: translate(-50%);
  background-color: #fff;
  width: 75%;
  padding: 30rpx 0 0;
  text-align: center;
}

.locationbox .box .title {
  font-size: 38rpx;
  margin-bottom: 10rpx;
}

.locationbox .box button,
.locationbox .box .bottons view {
  background-color: #fff;
  border-radius: 0;
  width: 50%;
  color: #888;
  font-size: 30rpx;
}

.content {
  color: #888;
  padding: 0 20rpx;
  font-size: 28rpx;
}

.bottons {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top: 2rpx solid #ccc;
  margin-top: 30rpx;
}

.button-hover {
  background: #fff;
  color: #f29219;
}

.locationbox .box .sure {
  border-left: 2rpx solid #888;
  color: #f29219;
  line-height: 3;
}

button[class="sure"]::after {
  border: 0;
}




