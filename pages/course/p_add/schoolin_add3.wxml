<view class="page">

	<block wx:if="{{userInfo.role==0}}">
		<view class="page_box ">
			<view class='page__bd_main'>
				<view class='add_padding'>
					<view class="spacing">学校名称:</view>
					<view class='text_col'>
						{{schoolModel.school ?schoolModel.school:schoolInfo.school }}
					</view>
				</view>
			</view>
			<view class='page__bd_main'>
				<view class='add_padding'>
					<view class="spacing">上课时间:</view>
					<view class='text_col'>
						{{model.oneStartTime}}
					</view>
				</view>
			</view>
			<view class='page__bd_main'>
				<view class='add_padding'>
					<view class="spacing">放学提醒:</view>
					<view class='text_col'>
						{{model.fiveStartTime}}
					</view>
				</view>
			</view>
			<view class='page__bd_main'>
				<view class='add_padding'>
					<view class="spacing">上课提醒:</view>
					<view class='text_col'>
						{{model.remindTxt}}
					</view>
				</view>
			</view>

			<!-- <view class='hint'>
		<text>班级名称：</text> {{setClassMsg.className?setClassMsg.className:courseInfo.className}}
	</view> -->
		</view>
	</block>
	<block wx:else>

		<view class='hint'>
			<text>学校名称：</text>{{schoolModel.school ?schoolModel.school:userInfo.workOrganizationName }}
		</view>
		<view class='hint'>
			<text>班级名称：</text> {{setClassMsg.className?setClassMsg.className:className}}
		</view>
	</block>

