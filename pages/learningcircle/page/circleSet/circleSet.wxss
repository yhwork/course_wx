/* pages/circle/circleSet/circleSet.wxss */
page{
  background: #F4F4F4;
}
.basic_info{
  display: flex;
  align-items: center;
  background: #fff;
  padding: 30rpx;
}
.photo_box{
  width: 84rpx;
  height: 84rpx;
  position: relative;
}
.photo{
  width: 84rpx;
  height: 84rpx;
  border-radius: 84rpx;
  border: 2rpx solid #f1f1f1;
}
.info_right{
  margin-left: 30rpx;
}
.basic_info .title{
  font-size:26rpx;
  color: #555;
}
.set_list{
  background: #fff;
  margin-top: 20rpx;
  padding: 0 30rpx;
}
.fa-angle-right {
  font-size: 50rpx;
  color: #c7c7cc;
}
.set_item{
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #F4F4F4;
  font-size: 28rpx;
}
.sex{
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}