/* pages/circle/cardTimeAlerting/cardTimeAlerting.wxss */
.card_container{
  background: #fff;
  min-height: 1180rpx; 
}
.card_time_title{
  height:88rpx;
  line-height: 88rpx;
  border-bottom:1px solid #e5e5e5;
  padding: 0 20rpx;
}
.card_time_left{
  float: left;
font-size:34rpx;
font-family:PingFangSC-Regular;
color:rgba(0,0,0,1);
}
.card_time_right{
  float: right;
}
.picker_label{
  float: left;
}
.arrow_img{
  float: right;
  margin-left: 20rpx;
}
.arrow_img image{
  width: 16rpx;
  height:27rpx;
}

.switch_title_tips{
  padding: 37rpx 20rpx;
  overflow: hidden;
}
.switch_title_tips image{
  float: left;
}
.switch_title_tips image{
  width: 32rpx;
  height:32rpx;
}
.switch_title_tips .text{
  float: left;
  width:625rpx;
height:80rpx;
font-size:26rpx;
font-family:PingFangSC-Regular;
color:rgba(136,136,136,1);
line-height:40rpx;
margin-left:20rpx; 
}