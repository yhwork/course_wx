/* 今日主题 */
page {
  background: #f4f4f4;
}

.circle_info {
  display: flex;
  align-items: center;
  padding-bottom: 20rpx;
  position: relative;
}

.circle_info::after {
  content: "";
  width: 100%;
  height: 2rpx;
  position: absolute;
  bottom: 0;
  background: #e5e5e5;
}

.page_top {
  background: #fff;
  padding: 40rpx 30rpx 30rpx;
}

.circle_info .info_img {
  width: 200rpx;
  height: 160rpx;
}

.info_right {
  margin-left: 30rpx;
}

.other_info {
  display: flex;
  align-items: center;
  color: #888;
  font-size: 26rpx;
  margin-top: 20rpx;
}

.clock_num {
  margin-left: 40rpx;
}

.zan image {
  width: 30rpx;
  height: 29rpx;
}

.zan text, .comment text {
  margin-left: 10rpx;
}

.comment image {
  width: 30rpx;
  height: 28rpx;
}

.comment {
  display: flex;
  align-items: center;
  margin-left: 60rpx;
}

.edi_set_admin {
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 22rpx;
  font-size: 26rpx;
  color: #888;
}

.editor, .set, .admin {
  display: flex;
  align-items: center;
}

.editor image {
  width: 29rpx;
  height: 36rpx;
  margin-right: 10rpx;
}

.set image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}

.admin image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}

.page_center {
  margin-top: 20rpx;
  padding-bottom: 90rpx;
  /* background: #fff; *//* padding:0 30rpx 30rpx 30rpx; */
}

.nav_tab {
  display: flex;
  justify-content: space-around;
  align-items: center;
  height: 90rpx;
  border-bottom: 1rpx solid #e5e5e5;
  color: #353535;
  font-size: 28rpx;
  background: #fff;
}

.active {
  color: #f29219;
  position: relative;
}
.active:after {
  content: '';
  position: absolute;
  bottom: -14rpx;
  left: 34%;
  width: 46rpx;
  height: 6rpx;
  border-radius: 0.08533333rem;
  background: #F29219;
}
.de_di_mem {
  /* padding:40rpx 30rpx; */
}

.hide {
  display: none;
}

.show {
  display: blcok;
}

.photo {
  width: 84rpx;
  height: 84rpx;
  border-radius: 84rpx;
}

.introduce {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  position: relative;
  
}
.introduce .sex{
position: absolute;
bottom:-5rpx;
left:50rpx;
z-index: 999;
}
.introduce .sex image{
  width: 40rpx;
  height:40rpx;
}

.intro_right {
  margin-left: 40rpx;
