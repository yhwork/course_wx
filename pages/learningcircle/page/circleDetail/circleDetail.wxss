/* pages/circle/circleDetail/circleDetail.wxss */
@import "../../../circle/circle.wxss";
.textover {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    text-overflow: ellipsis;
}
.circlelist_container{
 padding-left: 30rpx;
 padding-right: 30rpx;
}
.circle_lists{
  background: #fff;
  height:160rpx;
