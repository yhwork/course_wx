/* pages/circle/cardTimeSet/cardTimeSet.wxss */
.setprivacy_container{
  background: #fff;
   min-height: 1204rpx; 
}
.circle_type {
  padding: 30rpx;
  background: #fff;
  box-sizing:border-box;
}

.circle_type label {
  display: flex;
  align-items: center;
  margin-top: 30rpx;
}

.circle_type label text {
  font-size: 26rpx;
  color: #888;
  margin-left: 20rpx;
}

.circle_label {
  background: #fff;
  padding: 30rpx;
  position: relative;
}
.hide{
  display: none;
}
.show{
  display: block;
}
.circle_label::before {
  content: "";
  width: 690rpx;
  height: 1rpx;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  background: #e5e5e5;
}

.limit {
  font-size: 26rpx;
  color: #888;
  margin-left: 12rpx;
}

.label_cont {
  display: inline-block;
  width: 124rpx;
  height: 60rpx;
  line-height: 58rpx;
  text-align: center;
  border-radius: 26rpx;
  background: #f29219;
  color: #fff;
  margin-top: 26rpx;
}

.label_cont {
  margin-right: 60rpx;
  position: relative;
}
.privacy_text{
width: 100%;
height:34rpx;
font-size:26rpx;
font-family:PingFangSC-Regular;
color:rgba(136,136,136,1);
line-height:34rpx;
padding-left: 64rpx;
margin-top: 10rpx;
}

/* 打卡时间设置 */
.card_time_wrap{
  width: 100%;
  overflow: hidden;
  /* margin-top: 64rpx; */
}

.card_time_title{
overflow: hidden;

}
.card_time_title image{
  width: 32rpx;
  height:32rpx;
  margin-right: 20rpx;
}
.card_time_enter{
padding: 40rpx 160rpx;
background:rgba(255,255,255,1);
border-radius:8rpx;
border:1rpx solid rgba(229,229,229,1);
margin-top: 30rpx;
overflow: hidden;
}
.card_time_start{
  float: left;
  display: inline-block;
}
.card_time_middle{
  float: left;
  display: inline-block;
  margin-top: 50rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.card_time_middle image{
  width: 62rpx;
  height:62rpx;
}
.card_time_end{
  float: left;
  display: inline-block;
}
.time_tit{
width:105rpx;
height:26rpx;
font-size:26rpx;
font-family:PingFangSC-Regular;
color:rgba(242,146,25,1);
line-height:26rpx;
margin-bottom: 28rpx;
}
.time_number{
height:46rpx;
font-size:46rpx;
font-family:PingFangSC-Medium;
color:rgba(0,0,0,1);
line-height:46rpx;
}
.end_time{
  color:#000;
}

.picker{
    padding-left: 10px;
    background-color: #FFFFFF;
    color:#333;
}

.dao{
  width: 48rpx;
  height: 48rpx;
  border-radius: 48rpx;
  margin-bottom: 20rpx;
}

.cl_date{
  display: flex;
  align-items: flex-end;
  justify-content: space-around;
  width: 690rpx;
  border: 2rpx solid #e5e5e5;
  border-radius: 8rpx;
  padding: 20rpx 60rpx;
  box-sizing: border-box;
  margin: 20rpx auto;
}
.cl_date .title{
  margin-bottom: 30rpx;
  color: #f29219;
  font-size:26rpx;
}
.cl_time{
  margin-top: 20rpx;
}
.cl_date .start_date{
  text-align: right;
  color:#000;
  font-size:46rpx;
}
.save{
  width: 670rpx;
  height: 94rpx;
  line-height: 92rpx;
  text-align: center;
  position: absolute;
  bottom: 120rpx;
  left: 50%;
  transform: translateX(-50%);
  border-radius: 10rpx;
  background: #f29219;
  color: #fff;
}
.data_start{
  width: 50%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}
.time_start{
  line-height: 96rpx;
  font-size: 30rpx;
}
.data_box{
  display: flex;
  justify-content: space-between;
}
.date{
  color: #666;
  font-size: 26rpx;
}
.drop_down{
  width: 32rpx;
  height: 20rpx;
  position: absolute;
  right:10rpx;
  top: 37rpx;


}
.basic_info{
  display: flex;
  align-items: center;
  background: #fff;
  padding: 30rpx;
}
.photo_box{
  width: 84rpx;
  height: 84rpx;
  position: relative;
}
.photo{
  width: 84rpx;
  height: 84rpx;
  border-radius: 84rpx;
  border: 2rpx solid #f1f1f1;
}
.info_right{
  margin-left: 20rpx;
  display: flex;
  flex-direction: row;
}
.basic_info .title{
  font-size:32rpx;
  margin-left: 36rpx;
  color: #555;
}
.set_list{
  background: #fff;
  margin-top: 20rpx;
  padding: 0 30rpx;
}
.fa-angle-right {
  font-size: 50rpx;
  color: #c7c7cc;
}
.set_item{
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #F4F4F4;
  font-size: 28rpx;
}
.sex{
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}