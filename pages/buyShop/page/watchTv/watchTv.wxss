page {
  background-color: #fff;
}

.video_fix{
  height: 420rpx;
}
.video{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
}
.video-container{
  position: relative;
}
.video{
  width: 100%;
}

.cover-img{
  width: 100%;
  height: 420rpx;
}

.video-drag-disable{
  position: absolute;
  background: transparent;
  width: 100%;
  opacity: 0.5;
  height: 50px;
  bottom: 10px;
  right: 40px;
  /* background-color: red; */
}
.video_details{
  padding: 20rpx 30rpx;
  display: flex;
  flex-flow: column wrap;
}
.video_details_top{
  display: flex;
  justify-content:center;

  margin-bottom: 40rpx;
}

.video_catelog{
  display: flex;
  flex-flow: column wrap;
}
.video_catelog_title{
  display: flex;
  justify-content: space-between;
  color: #949494;
  height: 36rpx;
}
.video_catelog_title>view:nth-child(1){
  font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: 400;
	font-stretch: normal;
  line-height:36rpx;
	/* letter-spacing: -1px; */
	color: #333333;
}
.video_catelog_title>view:nth-child(2){
	font-family: PingFang-SC-Regular;
	font-size: 22rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height:36rpx;
	/* letter-spacing: -1rpx; */
	color: #666;
}
.video_details_prograss{
  font-family: PingFang-SC-Regular;
	font-size: 22rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 16rpx;
	letter-spacing: -1rpx;
	color: #bdbdbd;
}
.video_details_title{
  font-family: PingFang-SC-Bold;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: -2rpx;
	color: #323232;
  text-align: center;
}

.scroll-view_H{
  /* overflow-x: scroll; */
}
.video_catelog_width{
  width: 691rpx;
  margin-bottom: 10rpx;
  margin-top: 10rpx;
  padding-bottom: 10rpx;
  border-bottom: 1px solid rgba(0, 0, 0, .2)
}
.playImg{
  width: 88rpx;
  height: 88rpx;
  /* border: 1px solid red; */
}
.videoimage{
  width: 88rpx;
  height:88rpx;
  margin-left: 50%;
  margin-top: 30%;
  transform: translate(-50%,-50%);
  /* border: 1px solid red; */
}
 .video_details_img{
  width: 265rpx;
	height: 137rpx;
	/* background-color: #e7c60e; */
	border-radius: 10rpx 10rpx 0rpx 0rpx;
  /* margin-bottom: 15rpx; */
 }
 .video_details_img image{
    width: 100%;
   /* height: 100%; */
 }
 .video_catelog_box_item{
    margin: 20rpx;
   	width: 265rpx;
    height: 191rpx;
    background-color: #ffffff;
    box-shadow: 1rpx 1rpx 27rpx 0px 
      rgba(150, 149, 149, 0.26);
    border-radius: 10rpx;
    /* border: 1px solid red; */
 }
 .video_catelog_box_item>view:nth-child(2){
   text-align: center;
   width: 240rpx;
	/* height: 23rpx; */
	font-family: PingFang-SC-Regular;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: -1rpx;
	color: #333333;
 }
.video_catelog_box{
  display: flex;
  width: 100%;
}
.shop_details{
  display: flex;
  justify-content: space-between;
  margin-bottom: 15rpx;
}
.shop_details>view:nth-child(1){
	font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #333333;
}
.shop_details>view:nth-child(2){
	width: 29rpx;
	height: 37rpx;
	background-color: #008400;
}
.video_shop_details_txt{
  	font-family: PingFang-SC-Regular;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: -1rpx;
	color: #6868;
}
.video_shop_details_name{
  margin-top: 15rpx;
  font-size: 28rpx;
}
.video_shop_details_name>view:nth-child(1){
	font-family: PingFang-SC-Medium;
	font-size: 28rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: -1rpx;
	color: #333333;
}
.video_shop_details_name>view:nth-child(2){
  padding: 10rpx 15rpx;
	font-family: PingFang-SC-Regular;
	font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	color: #666666;
}

.messages{
  margin: 35rpx 0;
  text-align: center;
  	font-family: PingFang-SC-Regular;
	font-size: 22rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 16rpx;
	letter-spacing: -1rpx;
	color: #bdbdbd;
}
strong{
  margin: 0 4rpx;
  color: #ece02d
}





