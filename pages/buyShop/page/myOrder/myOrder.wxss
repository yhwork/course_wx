.content_info {
  width: 750rpx;
  background-color: #f2f2f2;
  overflow: hidden;
}
page{
  background-color: #f2f2f2;
}
.info_wrapbox {
  width: 710rpx;
  background-color: f2f2f2;
  margin: 0 auto;
  margin-top: 40rpx;
  padding: 0 20rpx;
  font-size: 30rpx;
  line-height: 40rpx;
}

.name {
  font-weight: bolder;
  padding-top: 30rpx;
  line-height: 30rpx;
}
.upimg{
  width:20rpx;
  height: 20rpx;
}
.manyinfo_box{
  display: flex;
}
.manyinfo_box{
  color: #306993;
}
.upimg{
  margin-left: 10rpx;
}
.addresscolor{
  color: #306993;
  font-weight: bolder;
}
.wrapbox_header{
  font-size: 28rpx;
  line-height-step: 25rpx;
  background-color: white;
width: 690rpx;
  padding: 10rpx 10rpx;
  margin-bottom: 20rpx;
}
.have_margin{
  margin-top: 30rpx;
}
.booking{
  width: 120rpx;
  height: 40rpx;
  background-color: #ff9800;
  color: white;
  line-height: 40rpx;
  border-radius: 10rpx;
  position: absolute;
  left:550rpx;
  justify-content: center;
  align-items: center;
  display: flex;

}
.btndispaly{
  display: flex;
}
.calendar{
  background-color: white;
}

